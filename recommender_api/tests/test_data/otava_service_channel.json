          "accessibilitySentences": [
            {
              "sentenceGroup": [
                {
                  "value": "Rutten till huvudingången",
                  "language": "sv"
                },
                {
                  "value": "The route to the main entrance",
                  "language": "en"
                },
                {
                  "value": "Reitti pääsisäänkäynnille",
                  "language": "fi"
                }
              ],
              "sentences": [
                {
                  "sentence": [
                    {
                      "value": "Bilplatsen för rörelsehindrade ligger utomhus under 10 m från ingången. Parkeringsrutans bredd är minst 3,6 m.",
                      "language": "sv"
                    },
                    {
                      "value": "The accessible parking space is located outdoors under 10 m from the entrance. The width of the parking space is at least 3.6 m.",
                      "language": "en"
                    },
                    {
                      "value": "Liikkumisesteisten 1 autopaikka sijaitsee ulkona alle 10 m sisäänkäynnistä. Pysäköintiruudun leveys on vähintään 3,6 m.",
                      "language": "fi"
                    }
                  ]
                },
                {
                  "sentence": [
                    {
                      "value": "Hållplatsen för skjutstrafik ligger i närheten av ingången, på en plats varifrån det är lätt att ta sig till trottoaren.",
                      "language": "sv"
                    },
                    {
                      "value": "The pick-up and drop-off area is located in the vicinity of the entrance, giving easy access to the pavement.",
                      "language": "en"
                    },
                    {
                      "value": "Saattoliikenteen pysähtymispaikka sijaitsee sisäänkäynnin läheisyydessä (etäisyys enintään 5 metriä), josta sisäänkäynnille pääsee siirtymään sujuvasti.",
                      "language": "fi"
                    }
                  ]
                },
                {
                  "sentence": [
                    {
                      "value": "Rutten till ingången är mjuk samt belyst.",
                      "language": "sv"
                    },
                    {
                      "value": "The route to the entrance is soft and illuminated.",
                      "language": "en"
                    },
                    {
                      "value": "Kulkureitti sisäänkäynnille on pehmeä sekä valaistu.",
                      "language": "fi"
                    }
                  ]
                }
              ]
            },
            {
              "sentenceGroup": [
                {
                  "value": "Huvudingången",
                  "language": "sv"
                },
                {
                  "value": "The main entrance",
                  "language": "en"
                },
                {
                  "value": "Pääsisäänkäynti",
                  "language": "fi"
                }
              ],
              "sentences": [
                {
                  "sentence": [
                    {
                      "value": "Ingången är lätt att urskilja och belyst. Ingången har ett tak.",
                      "language": "sv"
                    },
                    {
                      "value": "The entrance stands out clearly and is illuminated. There is a canopy above the entrance.",
                      "language": "en"
                    },
                    {
                      "value": "Sisäänkäynti erottuu selkeästi ja on valaistu. Sisäänkäynnin yläpuolella on katos.",
                      "language": "fi"
                    }
                  ]
                },
                {
                  "sentence": [
                    {
                      "value": "Dörrarna vid ingången är lätta att urskilja. Utanför dörren finns tillräckligt med fritt utrymme för att röra sig t.ex. med rullstol. Dörren är lätt att öppna för hand.",
                      "language": "sv"
                    },
                    {
                      "value": "The doors connected to the entrance stand out clearly. Outside the door there is sufficient room for moving e.g. with a wheelchair. The door opens easily manually.",
                      "language": "en"
                    },
                    {
                      "value": "Sisäänkäynnin ovet erottuvat selkeästi. Oven ulkopuolella on riittävästi vapaata tilaa liikkumiselle esim. pyörätuolin kanssa. Ovi avautuu käsin helposti.",
                      "language": "fi"
                    }
                  ]
                },
                {
                  "sentence": [
                    {
                      "value": "Vid ingången finns trösklar som är över 2 cm höga.",
                      "language": "sv"
                    },
                    {
                      "value": "The entrance has thresholds over 2 cm high.",
                      "language": "en"
                    },
                    {
                      "value": "Sisäänkäynnin yhteydessä on yli 2 cm korkeita kynnyksiä.",
                      "language": "fi"
                    }
                  ]
                }
              ]
            },
            {
              "sentenceGroup": [
                {
                  "value": "I lokalen",
                  "language": "sv"
                },
                {
                  "value": "In the facility",
                  "language": "en"
                },
                {
                  "value": "Sisätilat",
                  "language": "fi"
                }
              ],
              "sentences": [
                {
                  "sentence": [
                    {
                      "value": "Verksamhetsstället har 3 våningsplan.",
                      "language": "sv"
                    },
                    {
                      "value": "The customer service point has 3 floors.",
                      "language": "en"
                    },
                    {
                      "value": "Toimipisteessä on 3 kerrosta.",
                      "language": "fi"
                    }
                  ]
                },
                {
                  "sentence": [
                    {
                      "value": "I lokalen finns trösklar som är över 2 cm höga.",
                      "language": "sv"
                    },
                    {
                      "value": "The facility has thresholds over 2 cm high.",
                      "language": "en"
                    },
                    {
                      "value": "Tilassa on yli 2 cm korkeita kynnyksiä.",
                      "language": "fi"
                    }
                  ]
                },
                {
                  "sentence": [
                    {
                      "value": "Dörrarna i lokalen är lätta att urskilja.",
                      "language": "sv"
                    },
                    {
                      "value": "The doors in the facility stand out clearly.",
                      "language": "en"
                    },
                    {
                      "value": "Sisätilojen ovet erottuvat selkeästi.",
                      "language": "fi"
                    }
                  ]
                },
                {
                  "sentence": [
                    {
                      "value": "I verksamhetsstället finns en tillgänglig toalett på plan 1.",
                      "language": "sv"
                    },
                    {
                      "value": "The facility has an accessible toilet on floor 1.",
                      "language": "en"
                    },
                    {
                      "value": "Toimipisteessä on esteetön wc kerroksessa 1.",
