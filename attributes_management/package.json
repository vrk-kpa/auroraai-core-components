{
  "name": "attributes_management",
  "version": "0.0.1",
  "description": "Microservice to provide simple API between AuroraAI services and Y-alusta for attributes metadata",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -L --watch './**/*' --exec 'ENVIRONMENT=local ts-node --files' ./src/index.ts",
    "clean": "rm -f tsconfig.tsbuildinfo && rm -rf dist",
    "build": "yarn clean && node esbuild.js",
    "test": "jest $@",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'"
  },
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.23",
    "@types/jsonld": "^1.5.6",
    "@types/node": "15.3.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "esbuild": "0.12.18",
    "eslint": "7.26.0",
    "io-ts-reporters": "2.0.0",
    "jest": "27.0.4",
    "nodemon": "2.0.19",
    "prettier": "2.3.0",
    "supertest": "^6.2.3",
    "ts-jest": "27.0.3",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "base64url": "3.0.1",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.3",
    "fp-ts": "2.10.5",
    "helmet": "4.6.0",
    "http-status-codes": "2.1.4",
    "io-ts": "2.2.16",
    "io-ts-types": "0.5.16",
    "json-schema-ref-parser": "^9.0.9",
    "jsonld": "^5.2.0",
    "jsonschema": "^1.4.0",
    "node-cache": "^5.1.2",
    "node-cron": "3.0.0",
    "pg-promise": "10.10.2",
    "uuid": "8.3.2",
    "yaml": "1.10.2"
  }
}
