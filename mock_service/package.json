{
  "name": "mock_service",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/concurrently --kill-others \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "parcel watch src/index.html --dist-dir build --no-source-maps --public-url static",
    "dev:server": "nodemon -L --watch server.ts --exec 'ts-node' server.ts",
    "build": "parcel build src/index.html --no-source-maps --dist-dir server/build --public-url static; tsc -p tsconfig.server.json"
  },
  "dependencies": {
    "@types/url-join": "^4.0.1",
    "axios": "^0.24.0",
    "ejs": "3.1.7",
    "express": "4.18.2",
    "express-session": "1.17.2",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.1.4",
    "jose": "^4.13.1",
    "memorystore": "^1.6.7",
    "openid-client": "^5.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "styled-components": "5.3.0",
    "suomifi-ui-components": "7.0.0",
    "url-join": "^4.0.1",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.4",
    "@types/node": "16.11.26",
    "@types/react": "17.0.9",
    "@types/react-dom": "17.0.6",
    "@types/react-router-dom": "5.1.8",
    "concurrently": "6.2.0",
    "nodemon": "^2.0.19",
    "parcel": "2.7.0",
    "prettier": "2.3.2",
    "process": "^0.11.10",
    "ts-node": "10.1.0",
    "typescript": "4.3.2"
  }
}
